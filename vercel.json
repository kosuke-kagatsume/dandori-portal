{
  "public": true,
  "headers": [
    {
      "source": "/(.*\\.(jpg|jpeg|png|gif|svg|ico|webp))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/dandori-logo.jpg",
      "destination": "/dandori-logo.jpg"
    },
    {
      "source": "/dandori-logo-small.jpg",
      "destination": "/dandori-logo-small.jpg"
    },
    {
      "source": "/dandori-work-logo.jpg",
      "destination": "/dandori-work-logo.jpg"
    }
  ]
}